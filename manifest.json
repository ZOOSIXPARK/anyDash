{
  "manifest_version": 3,
  "name": "AnyLink7 커스텀로그 반영기",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "AnyLink7 커스텀로그 반영기"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["a.csv"],
      "matches": ["<all_urls>"]
    }
  ]
}
